{# templates/user/notifications.html.twig #}
<h1>Hello {{ user_first_name }}!</h1>
<p>You have {{ notifications|length }} new notifications.</p>
<p>You drank {{ cafe }}</p>

{% for user in users %}
    <li>{{ user }}</li>
{% endfor %}

{% for image in images %}
    <img src="{{ image }}" />
{% endfor %}